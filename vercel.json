{
    "version": 2,
    "name": "airport",
    "builds": [
        {
            "src": "airport/wsgi.py",
            "use": "@vercel/python",
            "config": { "maxLambdaSize": "15mb", "pythonVersion": "3.10" }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "airport/wsgi.py"
        }
    ]
}